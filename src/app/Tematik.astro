---
import ShowEntries from './component/ShowEntries.astro';
import DropDownYears from '../app/component/DropdownYears.astro';
import BreadCrumb from './component/BreadCrumb.astro';
import TableHeadTematik from '../app/Table/TableHeadTematik.astro';
---

<main class="p-5">
	<div class="flex items-start justify-between">
		<div class="grid">
			<h1 class="text-normal dark:text-neutral-200 text-2xl">Tematik</h1>
			<BreadCrumb />
		</div>
		<div>
			<DropDownYears />
		</div>
	</div>

	<div class="bg-white rounded-3xl dark:bg-[#151419]">
		<ShowEntries />
		<div>
			<table class="min-w-full mt-5">
				<!-- for thead -->
				<TableHeadTematik />
				<tbody class="p-10">
					<tr>
						<td class="p-4 text-sm text-gray-900 dark:text-white">1</td>
						<td class="p-4 text-sm text-gray-900 dark:text-white">Air Minum</td>
						<td class="p-4 text-sm text-gray-900 dark:text-white">10.000.000.000.000,00</td>
						<td class="p-4 text-sm text-gray-900 dark:text-white">10%</td>
						<td class="p-4 text-sm text-gray-900 dark:text-white">7.000.000.000.000,00</td>
						<td class="p-4 text-sm text-gray-900 dark:text-white">10%</td>
						<td class="p-4 text-sm text-gray-900 dark:text-white">5.000.000.000.000,00</td>
						<td class="p-4 text-sm text-gray-900 dark:text-white">9.5%</td>
					</tr>
					<tr>
						<td class="p-4 text-sm text-gray-900 dark:text-white">2</td>
						<td class="p-4 text-sm text-gray-900 dark:text-white">Stunting</td>
						<td class="p-4 text-sm text-gray-900 dark:text-white">8.000.000.000.000,00</td>
						<td class="p-4 text-sm text-gray-900 dark:text-white">9.5%</td>
						<td class="p-4 text-sm text-gray-900 dark:text-white">6.000.000.000.000,00</td>
						<td class="p-4 text-sm text-gray-900 dark:text-white">10%</td>
						<td class="p-4 text-sm text-gray-900 dark:text-white">4.500.000.000.000,00</td>
						<td class="p-4 text-sm text-gray-900 dark:text-white">9.5%</td>
					</tr>
					<tr>
						<td class="p-4 text-sm text-gray-900 dark:text-white">3</td>
						<td class="p-4 text-sm text-gray-900 dark:text-white">Perhutanan Sosial</td>
						<td class="p-4 text-sm text-gray-900 dark:text-white">7.000.000.000.000,00</td>
						<td class="p-4 text-sm text-gray-900 dark:text-white">9%</td>
						<td class="p-4 text-sm text-gray-900 dark:text-white">5.500.000.000.000,00</td>
						<td class="p-4 text-sm text-gray-900 dark:text-white">9.5%</td>
						<td class="p-4 text-sm text-gray-900 dark:text-white">4.000.000.000.000,00</td>
						<td class="p-4 text-sm text-gray-900 dark:text-white">9%</td>
					</tr>
					<tr>
						<td class="p-4 text-sm text-gray-900 dark:text-white">4</td>
						<td class="p-4 text-sm text-gray-900 dark:text-white">Infrastruktur</td>
						<td class="p-4 text-sm text-gray-900 dark:text-white">5.500.000.000.000,00</td>
						<td class="p-4 text-sm text-gray-900 dark:text-white">9.5%</td>
						<td class="p-4 text-sm text-gray-900 dark:text-white">4.300.000.000.000,00</td>
